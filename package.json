{
  "name": "pricing-calc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:newExcel": "rimraf ./src/testing/results_pleats_excel.csv && python ./src/testing/test_pleats_excel.py",
    "test:newApp": "rimraf ./src/testing/results_pleats_app.csv && python ./src/testing/test_pleats_calc.py",
    "test:newCompare": "rimraf ./src/testing/comparison_summary.csv && python ./src/testing/compare_results.py",
    "test:newAllPleats": "npm run test:newExcel && npm run test:newApp && npm run test:newCompare"
  },
  "dependencies": {
    "lucide-react": "^0.544.0",
    "papaparse": "^5.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/cli": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
